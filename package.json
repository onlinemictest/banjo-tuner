{
  "private": true,
  "name": "@onlinemictest/guitar",
  "version": "1.0.0",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "npm run postinstall:1; npm run postinstall:2",
    "postinstall:1": "for file in `ls .git-hooks`; do ln -sf ../../.git-hooks/${file} .git/hooks/${file}; done",
    "postinstall:2": "npm run post-commit",
    "pre-commit": "echo '<!--svg-->' | bin/into.js '<svg[\\s\\S]*</svg>' index.html && git add index.html",
    "post-commit": "cat guitar.svg | bin/into.js '<!--svg-->' index.html",
    "uninstall-hooks": "for file in `ls .git-hooks`; do rm .git/hooks/${file}; done"
  },
  "author": "Florian Klampfer <mail@qwtel.com> (https://qwtel.com/)",
